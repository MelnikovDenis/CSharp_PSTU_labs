@model IFormFile
<div class="forms">
      <h2 class="header">Выберите бинарный файл для загрузки</h2>
      <form asp-action="UploadBinaryFile" asp-controller="Home" method="post" enctype="multipart/form-data">
            <div class="forms_field">
                  <input type="file" name="uploadedFile" value=@Model/><br>
            </div>
            <div class="forms_field">
                  <input type="submit" value="Загрузить" />
            </div>
      </form>
</div>